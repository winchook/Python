var v = 123;
function foo(){
    var v = 456;
    function inner(){
        console.log(v)
    }
    return inner
}

result = foo();
console.log(result());

# 问输出结果：

Name='root';
Age = 18;
function Foo(name,age){
    this.Name = name;
    this.Age = age;
    this.Func = function(){
        console.log(this.Name,this.Age);
        (function(){
             console.log(this.Name,this.Age);
        })();
    }
}
obj = new Foo('winchoo',666);
obj.Func();
# 问输出结果：